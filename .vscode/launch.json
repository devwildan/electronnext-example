{
    "version": "0.2.0",
    "configurations": [ 
      {
        "name": "Electron main process",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
        "windows": {
          "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
        },
        "program": "${workspaceRoot}/main/index.js",
        "protocol": "inspector"
      } 
    ]
  }